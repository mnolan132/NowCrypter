{
    "manifest_version": 3,
    "name": "NowCrypter",
    "version": "2.0.0",
    "action": {
        "default_popup": "index.html",
        "default_logo": "logo-thumbnail.png"
    },
    "permissions": [
        "scripting"
    ],
    "host_permissions": [
        "http://*/*",
        "https://*/*"
    ],
    "icons":{
        "32": "logo-thumbnail.png"
    }
    
}